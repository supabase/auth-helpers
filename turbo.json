{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:example": {
      "dependsOn": ["^build", "^build:example"],
      "outputs": ["dist/**", ".next/**"]
    },
    "clean:all": {
      "dependsOn": ["^clean:all"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    "tsconfig.json",
    ".env.*"
  ]
}
